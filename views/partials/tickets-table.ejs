<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h4 class="panel-title">
          <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" class="">
            Bug Tickets
          </a>
        </h4>
      </div>
      <div id="collapseOne" class="panel-collapse collapse in" aria-expanded="true" style="">
        <!-- /.panel-heading -->
        <div class="panel-body">
          <div class="loader"></div>
          <div class="alert alert-success alert-dismissable" id="action-alert-success">
            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
            <span class="alert-content"></span>
          </div>
          <div class="alert alert-danger alert-dismissable" id="action-alert-error">
            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
            <span class="alert-content"></span>
          </div>
          <table width="100%" class="table table-striped table-bordered table-hover" id="tickets-list">
            <thead>
              <tr role="row">
                <th>#</th>
                <th>Title</th>
                <th>Description</th>
                <th>Category</th>
                <th>Related Feature</th>
                <th>Department</th>
                <th>Reported By</th>
                <th>Created At</th>
              </tr>
            </thead>
            <tbody>
              <% for(var i=0; i<data.length; i++) {%>
              <tr class="odd gradeX" role="row">
                <td class="center">
                  <%= data[i]["id"] %>
                </td>
                <td class="center">
                  <%= data[i]["title"] %>
                </td>
                <td class="center">
                  <%= data[i]["description"] %>
                </td>
                <td class="center">
                  <%= data[i]["category"] %>
                </td>
                <td class="center">
                  <%= data[i]["relatedFeature"] %>
                </td>
                <td class="center">
                  <%= data[i]["department"] %>
                </td>
                <td class="center">
                  <%= data[i]["reporter"] %>
                </td>
                <td class="center">
                  <%= data[i]["createdAt"] %>
                </td>
              </tr>
              <% } %>
            </tbody>
          </table>
          <!-- /.table-responsive -->
        </div>
      </div>
    </div>
    <!-- /.panel-body -->
  </div>
  <!-- /.panel -->
</div>
<!-- /.col-lg-12 -->